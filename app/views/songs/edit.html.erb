<h1>Edit Song</h1>

<%= form_for @song do |f| %>
  <%= f.label :title %> <br>
  <%= f.text_field :title, @song.title %> <br>

  <%= f.label :artist_name, "artist" %> <br>
  <%= f.text_field :artist_name, @song.artist_name %> <br>

  <%= f.label :genre %> <br>
  <%= f.text_field :genre, @song.genre %> <br>

  <%= f.label :released %> <br>
  <%= f.text_field :released, @song.released %> <br>

  <%= f.label :release_year %> <br>
  <%= f.text_field :release_year, @song.release_year %> <br>

  <%= f.submit %>
<% end %>
